{
  "type": "lib",
  "config": "air",
  "compilerOptions": {
    "source-path": [
      "src"
    ],
    "include-sources": [
      "src"
    ],
    "library-path": [
      "libs"
    ],
    "include-classes": [
      "air.net.websockets.WebSocketServer",
      "air.net.websockets.events.ServerEvent",
      "air.net.websockets.events.ClientEvent"
    ],
    "default-size": {
      "width": 20,
      "height": 20
    },
    "strict": true,
    "debug": false,
    "directory": true,
    "output": "bin"
  }
}