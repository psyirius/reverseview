{
  "name": "@rvw/builder",
  "version": "1.0.0",
  "description": "Specialized typescript compiler built for the air runtime",
  "type": "module",
  "license": "MIT",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "bin": {
    "build-rvw": "./dist/cli.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsup",
    "bld": "tsx ./src/build.ts",
    "xcm": "tsx ./src/xompile.ts",
    "svx": "tsx ./src/svempile.ts",
    "bxd": "node src/index.js -w ../../apps/desktop -p tsconfig.app.json",
    "par": "tsx ./src/parcel.ts",
    "mb": "microbundle build -i input/main.tsx -o out -f umd --pkg-main false --compress false --target web --name abc --visualize"
  },
