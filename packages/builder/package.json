{
  "name": "@rvw/builder",
  "version": "1.0.0",
  "description": "Specialized typescript compiler built for the air runtime",
  "type": "module",
  "private": true,
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "scripts": {
    "bld": "tsx ./src/build.ts",
    "svx": "tsx ./src/svempile.ts",
    "build": "node src/index.js -w ../../apps/desktop -p tsconfig.app.json",
    "par": "tsx ./src/parcel.ts",
    "mb": "microbundle build -i input/main.tsx -o out -f umd --pkg-main false --compress false --target web --name abc --visualize"
  },
  "dependencies": {
    "@farmfe/core": "^1.3.20",
    "@parcel/core": "^2.12.0",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@rslib/core": "^0.0.6",
    "@rspack/core": "^1.0.5",
    "@rvw/parcel-config": "workspace:*",
    "@swc/core": "^1.7.26",
    "@swc/helpers": "^0.5.13",
    "@swc/plugin-transform-imports": "^3.0.2",
    "acorn": "^8.12.1",
    "commander": "^12.1.0",
    "esbuild": "^0.23.1",
    "microbundle": "^0.15.1",
    "rollup": "^4.21.3",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "svelte": "^3.59.2",
    "es6-shim": "^0.35.8",
    "es-map": "^1.0.6",
    "es-set": "^1.1.2",
    "function-bind": "^1.1.2",
    "terser": "^5.33.0",
    "ts-morph": "^23.0.0",
    "tslib": "^2.7.0",
    "typescript": "^5.6.2"
  },
  "devDependencies": {
    "core-js": "^3.38.1",
    "@rollup/plugin-legacy": "^3.0.2",
    "@rollup/plugin-strip": "^3.0.4",
    "@rollup/plugin-swc": "^0.4.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@umijs/mako": "^0.8.11",
    "inferno": "^8.2.3",
    "inferno-create-element": "^8.2.3",
    "lodash": "^4.17.21",
    "million": "^3.1.11",
    "preact": "^10.24.0",
    "rollup-plugin-cleanup": "^3.2.1",
