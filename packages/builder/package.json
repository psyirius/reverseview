{
  "name": "@rvw/builder",
  "version": "1.0.0",
  "description": "Specialized typescript compiler built for the air runtime",
  "type": "module",
  "private": true,
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "scripts": {
    "bld": "tsx ./src/build.ts",
    "build": "node src/index.js -w ../../apps/desktop -p tsconfig.app.json",
    "par": "tsx ./src/parcel.ts",
    "mb": "microbundle build -i input/main.tsx -o out -f umd --pkg-main false --compress false --target web --name abc --visualize"
  },
  "dependencies": {
    "@rvw/parcel-config": "workspace:*",
    "microbundle": "^0.15.1",
    "typescript": "^5.6.2",
    "ts-morph": "^23.0.0",
    "tslib": "^2.7.0",
    "commander": "^12.1.0",
    "acorn": "^8.12.1",
    "@farmfe/core": "^1.3.20",
    "@rspack/core": "^1.0.5",
    "@rslib/core": "^0.0.6",
    "@swc/core": "^1.7.26",
    "@swc/helpers": "^0.5.13",
    "@swc/plugin-transform-imports": "^3.0.2",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-typescript": "^11.1.6",
    "rollup-plugin-typescript2": "^0.36.0",
    "rollup-plugin-postcss": "^4.0.2",
    "@parcel/core": "^2.12.0",
    "esbuild": "^0.23.1",
    "rollup": "^4.21.3",
    "terser": "^5.32.0"
  },
  "devDependencies": {
    "vite": "^5.4.5",
    "tsup": "^8.2.4",
    "preact": "^10.24.0",
    "lodash": "^4.17.21",
    "inferno": "^8.2.3",
    "million": "^3.1.11",
    "solid-js": "^1.8.22",
    "inferno-create-element": "^8.2.3"
  }
}
