{
  "name": "@rvw/flash-bootloader",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "files": [
    "bin"
  ],
  "scripts": {
    "build": "pnpm -F=. run --parallel \"/^build:.*/\"",
    "build:as3": "dotenvx run --verbose -- asconfigc --verbose",
    "build_:swf": "amxmlc -o \"bin/AppEntry.swf\" \"src/AppEntry.as\"",
    "dump:swf": "swfdump \"bin/AppEntry.swf\"",
    "dump:swf-abc": "swfdump -abc \"bin/AppEntry.swf\"",
    "clean": "rimraf -g \"./bin/*\""
  },
  "devDependencies": {
    "asconfigc": "^1.8.1"
  }
}
