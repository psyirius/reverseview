{
  "name": "@rvw/desktop",
  "version": "8.5.0",
  "description": "Rebirth of the old-school VerseVIEW 8",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20",
    "pnpm": ">=9"
  },
  "scripts": {
    "dev": "pnpm run build && pnpm run debug",
    "debug": "adl -profile extendedDesktop ./application.xml ./.air",
    "start": "adl -profile extendedDesktop -nodebug ./application.xml ./.air",
    "build": "concurrently \"pnpm run build:src\" \"pnpm run build:styles\" \"pnpm run build:views\" \"pnpm run build:remote\" \"pnpm run build:static\"",
    "build:src": "tsc -p tsconfig.app.json",
    "build:views": "node build.views.js",
    "build:static": "node build.static.js",
    "build:parcel": "node build.parcel.js",
    "build:styles": "node build.styles.js",
    "build:remote": "node build.remote.js",
    "watch:src": "tsc -p tsconfig.app.json --watch",
    "clean": "rimraf -g \"./.air\" \"./dist\"",
    "package": "pnpm run build && node tools/releng/package.js"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "@parcel/config-default": "^2.12.0",
    "@parcel/core": "^2.12.0",
    "@parcel/plugin": "^2.12.0",
    "@parcel/utils": "^2.12.0",
    "@parcel/logger": "^2.12.0",
    "@parcel/watcher": "^2.4.1",
    "@parcel/workers": "^2.12.0",
    "@parcel/events": "^2.12.0",
    "@parcel/cache": "^2.12.0",
    "@parcel/fs": "^2.12.0",
    "@types/dojo": "^1.9.48",
    "@types/jquery": "^3.5.30",
    "@types/lodash": "^4.17.7",
    "@types/node": "^20.14.14",
    "@types/sizzle": "^2.3.4",
    "@types/yui": "^3.14.31",
    "alloy-ui": "^3.1.0-deprecated.83",
    "almond": "^0.3.3",
    "asconfigc": "^1.8.1",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.3",
    "cajon": "^0.4.4",
    "cssnano": "^7.0.4",
    "dijit": "^1.17.3",
    "dojo": "^1.17.3",
    "dojo-dstore": "^1.2.1",
    "dojox": "^1.17.3",
    "glob": "^11.0.0",
    "handlebars": "^4.7.8",
    "ignore": "^5.3.1",
    "jquery": "^3.7.1",
    "lodash": "^4.17.21",
    "map.groupby": "^1.0.3",
    "parcel": "^2.12.0",
    "postcss": "^8.4.41",
    "postcss-alias": "^2.0.0",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.1.0",
    "postcss-load-config": "^6.0.1",
    "postcss-nested": "^6.2.0",
    "postcss-preset-env": "^10.0.0",
    "preact": "^10.23.1",
    "prim": "^1.0.0",
    "recache": "^0.6.0",
    "sass": "^1.77.8",
    "stylelint": "^16.8.1",
    "tailwindcss": "^3.4.9",
    "teoria": "^2.5.0",
    "tslib": "^2.6.3",
    "typescript": "~5.4.5",
    "yui": "^3.18.1"
  }
}
