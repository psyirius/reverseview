{
  "name": "@rvw/desktop",
  "version": "8.5.0",
  "description": "Rebirth of the old-school VerseVIEW 8",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20",
    "pnpm": ">=9"
  },
  "dependencies": {
    "@rvw/bible-db": "workspace:*",
    "@rvw/flash-bootloader": "workspace:*",
    "@rvw/flash-support": "workspace:*",
    "@rvw/remote": "workspace:*",
    "@rvw/songs-db": "workspace:*",
    "@typescript/lib-scripthost": "workspace:@rvw/air-types@*"
  },
  "scripts": {
    "dev": "pnpm run build && pnpm run debug",
    "debug": "adl ./application.xml ./.air",
    "start": "adl -nodebug ./application.xml ./.air",
    "build": "dotenvx run -- pnpm -F=. run --parallel \"/^build:.*/\"",
    "buildx:rollup": "tsx ./build.rollup.ts",
    "buildx:src": "tsc -p tsconfig.app.json",
    "watchx:src": "tsc -p tsconfig.app.json --watch",
    "buildx-trace-resolutions:src": "tsc --traceResolution -p tsconfig.app.json",
    "build-new:src": "build-rvw",
    "build:src": "node build.src.js -p tsconfig.app.json && build-rvw",
    "build-trace-resolutions:src": "node build.src.js -tr -p tsconfig.app.json",
    "build:static": "node build.static.js",
    "build:styles": "node build.styles.js",
    "build:remote": "node build.remote.js",
    "build:support": "node build.support.js",
    "build:databases": "node build.databases.js",
    "build:bootloader": "node build.bootloader.js",
    "clean": "rimraf -g \"./.air\" \"./dist\"",
    "package": "pnpm run build && node tools/releng/package.js"
  },
  "devDependencies": {
    "@rvw/builder": "workspace:*",
    "@million/lint": "^1.0.0-rc.84",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-html": "^1.0.4",
    "@rollup/plugin-legacy": "^3.0.2",
    "@rollup/plugin-multi-entry": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.4",
    "@rollup/plugin-strip": "^3.0.4",
    "@rollup/plugin-swc": "^0.4.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.0",
    "@rollup/plugin-virtual": "^3.0.2",
    "@rollup/pluginutils": "^5.1.1",
    "@shockpkg/ria-packager": "^4.0.4",
    "@swc/core": "^1.7.26",
    "@swc/helpers": "^0.5.13",
    "@types/dojo": "^1.9.48",
    "@types/jquery": "^3.5.30",
    "@types/lodash": "^4.17.7",
    "@types/sizzle": "^2.3.8",
    "@types/yui": "^3.14.31",
    "alloy-ui": "^3.1.0-deprecated.83",
    "almond": "^0.3.3",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.7",
    "cajon": "^0.4.4",
    "mitt": "^3.0.1",
    "dlv": "^1.1.3",
    "emotion": "^11.0.0",
    "unistore": "^3.5.2",
    "commander": "^12.1.0",
    "cssnano": "^7.0.6",
    "dojo": "^1.17.3",
    "dojox": "^1.17.3",
    "dijit": "^1.17.3",
    "unstorage": "^1.12.0",
    "dojo-dstore": "^1.2.1",
    "dts2as": "^0.13.0",
    "ejs": "^3.1.10",
    "eslint": "^9.11.0",
    "glob": "^11.0.0",
    "handlebars": "^4.7.8",
    "ignore": "^6.0.2",
    "jquery": "^3.7.1",
    "jsdoc": "^4.0.3",
    "json5": "^2.2.3",
    "lodash": "^4.17.21",
    "map.groupby": "^1.0.3",
    "million": "^3.1.11",
    "postcss": "^8.4.47",
    "postcss-alias": "^2.0.0",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.1.0",
    "postcss-load-config": "^6.0.1",
    "postcss-nested": "^6.2.0",
    "postcss-preset-env": "^10.0.5",
    "preact": "^10.24.0",
    "prim": "^1.0.0",
    "pug": "^3.0.3",
    "recache": "^0.6.0",
    "rollup": "^4.22.4",
    "rollup-plugin-amd": "^4.0.0",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.79.3",
    "vanilla-picker": "^2.12.3",
    "stylelint": "^16.9.0",
    "tailwindcss": "^3.4.12",
    "tslib": "^2.7.0",
    "tsup": "^8.3.0",
    "typescript": "~5.4.5",
    "yaml": "^2.5.1",
    "yui": "^3.18.1"
  }
}
